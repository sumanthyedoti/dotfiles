{
  "defaultMode": "acceptEdits",
  "enabledPlugins": {
    "backend-architect@cc-marketplace": true,
    "claude-opus-4-5-migration@claude-code-plugins": true,
    "code-architect@cc-marketplace": true,
    "code-review@claude-plugins-official": true,
    "code-simplifier@claude-plugins-official": true,
    "commit-commands@claude-plugins-official": true,
    "compound-engineering@every-marketplace": true,
    "context7-docs-fetcher@cc-marketplace": true,
    "context7@claude-plugins-official": true,
    "data-privacy-engineer@cc-marketplace": true,
    "deployment-engineer@cc-marketplace": true,
    "dev-browser@dev-browser-marketplace": false,
    "explanatory-output-style@claude-code-plugins": false,
    "feature-dev@claude-code-plugins": true,
    "feature-dev@claude-plugins-official": true,
    "frontend-design@claude-code-plugins": true,
    "frontend-design@claude-plugins-official": true,
    "frontend-developer@cc-marketplace": true,
    "github@claude-plugins-official": false,
    "hookify@claude-plugins-official": false,
    "learning-output-style@claude-plugins-official": true,
    "lua-lsp@claude-plugins-official": true,
    "pr-review-toolkit@claude-plugins-official": true,
    "ralph-loop@claude-plugins-official": false,
    "security-guidance@claude-plugins-official": true,
    "serena@claude-plugins-official": true,
    "studio-producer@cc-marketplace": true,
    "superpowers@claude-plugins-official": true,
    "typescript-lsp@claude-plugins-official": true,
    "ui-designer@cc-marketplace": true,
    "ux-researcher@cc-marketplace": true,
    "web-dev@cc-marketplace": true
  },
  "hooks": {
    "Notification": [
      {
        "hooks": [
          {
            "command": "workmux set-window-status waiting",
            "type": "command"
          }
        ],
        "matcher": "permission_prompt|elicitation_dialog"
      }
    ],
    "PostToolUse": [
      {
        "hooks": [
          {
            "command": "workmux set-window-status working",
            "type": "command"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "command": "workmux set-window-status done",
            "type": "command"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "command": "workmux set-window-status working",
            "type": "command"
          }
        ]
      }
    ]
  },
  "mcpServers": {
    "playwriter": {
      "args": [
        "-y",
        "playwriter@latest"
      ],
      "command": "npx"
    }
  },
  "plugins": {
    "commit-commands": {
      "includeCoAuthor": false
    }
  },
  "statusLine": {
    "command": "npx -y @owloops/claude-powerline@latest --style=powerline",
    "type": "command"
  }
}
