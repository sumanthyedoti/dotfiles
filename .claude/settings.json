{
  "model": "claude-opus-4-5",
  "statusLine": {
    "type": "command",
    "command": "npx -y @owloops/claude-powerline@latest --style=powerline"
  },
  "enabledPlugins": {
    "frontend-design@claude-plugins-official": true,
    "typescript-lsp@claude-plugins-official": true,
    "feature-dev@claude-plugins-official": true,
    "code-simplifier@claude-plugins-official": true,
    "github@claude-plugins-official": false,
    "context7@claude-plugins-official": true,
    "code-review@claude-plugins-official": true,
    "commit-commands@claude-plugins-official": true,
    "security-guidance@claude-plugins-official": true,
    "pr-review-toolkit@claude-plugins-official": true,
    "hookify@claude-plugins-official": false,
    "learning-output-style@claude-plugins-official": true,
    "serena@claude-plugins-official": true,
    "ralph-loop@claude-plugins-official": false,
    "claude-opus-4-5-migration@claude-code-plugins": true,
    "explanatory-output-style@claude-code-plugins": false,
    "compound-engineering@every-marketplace": true,
    "ui-designer@cc-marketplace": true,
    "dev-browser@dev-browser-marketplace": false
  },
  "mcpServers": {
    "playwriter": {
      "command": "npx",
      "args": [
        "-y",
        "playwriter@latest"
      ]
    }
  },
  "defaultMode": "acceptEdits",
  "plugins": {
    "commit-commands": {
      "includeCoAuthor": false
    }
  }
}
